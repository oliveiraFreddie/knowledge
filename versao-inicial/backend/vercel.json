{
    "version": 2,
    "builds": [
        {
          "src": "/migrations/**",
          "use": "@now/static"
        },
        {
          "src": "/index.js",
          "use": "@vercel/node"
        }
     ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "/index.js"
        }
    ]

}