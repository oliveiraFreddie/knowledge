{
    "version": 2,
    "builds": [
       {
        "src": "/index.js",
        "use": "@vercel/node"
       } 
    ],
    "routes": [
        {
            "src": "/config/routes.js",
            "dest": "/index.js"
        },
        {
            "src": "(.*)",
            "dest": "/index.js"
        }
    ]
}
